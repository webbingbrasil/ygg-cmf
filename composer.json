{
    "name": "webbingbrasil/ygg-cmf",
    "description": "One CMF to support all our projects, just as yggdrasil supports all worlds.",
    "authors": [
        {
            "name": "Danilo Andrade",
            "email": "danilo@webbingbrasil.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-intl": "*",
        "ext-json": "*",
        "laravel/framework": "^6.2",
        "intervention/image": "^2.4",
        "ralouphie/mimey": "^2.1",
        "bensampo/laravel-enum": "^1.33"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "doctrine/dbal": "~2.5",
        "orchestra/testbench": "~4.5"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Ygg\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ],
        "psr-4": {
            "Ygg\\Tests\\": "tests/",
            "App\\": "install-stubs/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Dashboard": "Ygg\\Support\\Facades\\Dashboard"
            },
            "providers": [
                "Ygg\\Platform\\Providers\\YggServiceProvider"
            ]
        }
    }
}
